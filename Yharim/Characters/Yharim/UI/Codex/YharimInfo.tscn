[sub_resource type="Shader" id=5]
code = "shader_type canvas_item;

uniform float wave_speed = 1.0;
uniform float wave_amplitude = 0.1;
uniform float wave_frequency = 1.0;

void fragment() {
	vec2 uv = UV;
	
	// Calculate the wave offset
	float wave_offset = sin(uv.x * wave_frequency + TIME * wave_speed) * wave_amplitude;

	// Apply the wave offset to the UV coordinates
	uv.y += wave_offset;

	// Sample the texture using the modified UV coordinates
	COLOR = texture(TEXTURE, uv);
}

"

